<%- include("header", {title: title}); %>
<article class="">
<h1>Skapa ny order</h1>
<form class="label-left" method="post">
    <fieldset>
        <legend>Välj kund</legend>

        <label>Kundregister</label><br>
        <select class="" name="kundnr">
            <option value="0">Välj i listan</option>
            <% for (const row of res) { %>
                <option value="<%= row.kundnr %>"><td><%= row.fornamn %></option>
            <% }; %>
        </select>

        <input type="submit" name="doit" value="Skapa">
    </fieldset>
</form>
<p>
    <a href="index">Index</a>
</p>
</article>
<%- include("footer"); %>
